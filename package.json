{
  "name": "tw-stock-health-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "selfcheck": "npx tsx scripts/self_check_scores.ts",
    "selfcheck:fundamental": "npx tsx scripts/self_check_fundamental_cap.ts",
    "selfcheck:names": "npx tsx scripts/self_check_company_names.ts",
    "selfcheck:news": "npx tsx scripts/self_check_news.ts",
    "selfcheck:fallback": "ts-node --transpile-only -P scripts/tsconfig.selfcheck.json scripts/self_check_finmind_fallback.ts",
    "selfcheck:prob": "ts-node --transpile-only -P scripts/tsconfig.selfcheck.json scripts/self_check_probabilities.ts",
    "selfcheck:strategy": "ts-node --transpile-only -P scripts/tsconfig.selfcheck.json scripts/self_check_strategy.ts",
    "selfcheck:consistency": "ts-node --transpile-only -P scripts/tsconfig.selfcheck.json scripts/self_check_consistency.ts",
    "selfcheck:all": "npm run selfcheck:names && npm run selfcheck:fundamental && npm run selfcheck:news",
    "report:daily": "npx tsx scripts/generateDailyReport.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}